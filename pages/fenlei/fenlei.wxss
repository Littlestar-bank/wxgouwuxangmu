background-color: #f2f2f2;
border-top: 1px solid #d8d8d8;
border-bottom: 1px solid #d8d8d8;
width: 100%;
box-sizing: border-box;

}
.shousuo-2 {
  height: 30px;
background-color: #fff;
text-align: center;
line-height: 30px;
border-radius: 5rpx;
